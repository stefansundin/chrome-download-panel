{
  "manifest_version": 3,
  "name": "Download panel",
  "version": "0.4.1",
  "description": "Adds a download panel to the Developer Tools.",
  "homepage_url": "https://github.com/stefansundin/devtools-download-panel",
  "author": "Stefan Sundin",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "256": "img/icon256.png"
  },
  "background": {
    "scripts": ["sw.js"]
  },
  "devtools_page": "devtools.html",
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_icon": {
      "38": "img/icon38.png"
    },
    "default_popup": "options.html"
  },
  "permissions": [
    "downloads",
    "clipboardRead",
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "download-panel@firefox.stefansundin.com",
      "strict_min_version": "115.0"
    }
  }
}
